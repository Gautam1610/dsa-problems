Brute Force =
  find the sum of all subarrays
  print the maximum sum
      int maxSubArray(vector<int>& nums) {
        int result=nums[0];
        for(int i=0;i<nums.size();i++){
            int current_sum=0;
            for(int j=i;j<nums.size();j++){
                current_sum+=nums[j];
                result=max(current_sum,result);
            }
        }
        return result;  
    }
    
